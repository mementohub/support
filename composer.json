{
  "name": "imemento/support",
  "description": "Support helpers",
  "homepage": "https://gitlab.com/imemento/composer/packages/support",
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "league/fractal": "^0.17.0"
  },
  "autoload": {
    "psr-4": {
      "iMemento\\Support\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "iMemento\\Support\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "repositories": [
    {"type": "composer", "url": "https://repo.packagist.com/imemento/"},
    {"packagist.org": false}
  ],
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  }
}
