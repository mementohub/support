{
  "name": "imemento/support",
  "description": "Support helpers",
  "homepage": "https://gitlhub.com/mementohub/support",
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "league/fractal": "^0.17 || ^0.18 || ^0.19"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0"
  },
  "autoload": {
    "psr-4": {
      "iMemento\\Support\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "iMemento\\Support\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
